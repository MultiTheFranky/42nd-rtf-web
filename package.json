{
    "name": "42ndrtf-web",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "start-to-test": "pnpm start",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "prepare": "husky",
        "precommit": "pnpm lint && pnpm prettier",
        "prepush": "pnpm build",
        "lint": "eslint src",
        "prettier": "prettier --write . --plugin=prettier-plugin-astro",
        "typecheck": "tsc --noEmit",
        "test": "concurrently \"pnpm typecheck\" \"pnpm lint\" -n \"Typecheck,Lint\"",
        "clean": "rimraf .astro dist node_modules && pnpm install"
    },
    "engines": {
        "npm": "please-use-pnpm",
        "node": ">=18",
        "pnpm": ">=8",
        "yarn": "please-use-pnpm"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@astrojs/cloudflare": "^12",
        "@astrojs/react": "^4.1.2",
        "@astrojs/tailwind": "^5",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/material": "^6.3.0",
        "@nanostores/react": "^0.8.4",
        "@react-spring/web": "^9.7.5",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "appwrite": "^16.1.0",
        "astro": "^5.1.1",
        "nanostores": "^0.11.3",
        "react": "^18.3.1",
        "react-d3-tree": "^3.6.2",
        "react-dom": "^18.3.1",
        "react-flippy": "^1.1.0",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.14"
    },
    "devDependencies": {
        "@commitlint/cli": "19.6.1",
        "@commitlint/config-conventional": "19.6.0",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.17.0",
        "@typescript-eslint/parser": "^8.18.2",
        "concurrently": "^9.1.2",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-astro": "^1.3.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-unicorn": "^56.0.1",
        "husky": "^9.1.6",
        "prettier": "^3.3.3",
        "prettier-eslint": "^16.3.0",
        "prettier-plugin-astro": "^0.14.1",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.2"
    }
}
