---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import Head from "./Head.astro";
import Orbat from "../components/Orbat.astro";

interface Props {
    title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
    <Head title={title} />
    <body class="h-full">
        <Header title={title} />
        <slot />
        <Orbat />
        <slot />
        <Footer />
    </body>
</html>
